<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="uavforge.uavforge.MapPlanning">
    <RelativeLayout
        android:id="@+id/TopPart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/MiddlePart">
        <WebView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/GoogleMapView"
            android:layout_alignParentBottom="true" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/MiddlePart"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_above="@+id/BottomPart"
        android:background="#000000">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_height="match_parent"
                android:id="@+id/TextView01" android:text="Action"

                android:layout_weight="1"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/TextView02" android:text="Long"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView03" android:text="E/W"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#cac9c9"
                android:textSize="15dp"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView04" android:text="Lat"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView05" android:text="N/S"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView06" android:text="Behavior"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#cac9c9"
                android:textSize="15dp"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/BottomPart"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:paddingTop="2dp">
            <TextView
                android:layout_height="match_parent"
                android:id="@+id/TextView001"
                android:layout_weight="1"
                android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/TextView002"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView003"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#cac9c9"
                android:textSize="15dp"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView004"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#dcdcdc"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView005"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#d3d3d3"
                android:textColor="#000000"
                android:gravity="center"/>
            <TextView
                android:id="@+id/TextView006"
                android:layout_height="match_parent"
                android:layout_weight="1" android:background="#cac9c9"
                android:textSize="15dp"
                android:textColor="#000000"
                android:gravity="center"/>
        </TableRow>
    </RelativeLayout>
</RelativeLayout>
